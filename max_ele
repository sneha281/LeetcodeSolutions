

#include <iostream>

using namespace std;

int main()
{
    int n,cnt=1,p;
    cout<<"enter size";
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    for(int i =0;i<n;i++){
        if(a[i]==a[i+1]){
            cnt++; 
            p = a[i];
        }
      
    }
    cout<<"maxx count of element  "<<cnt<<p;

    return 0;
}
