

#include <iostream>

using namespace std;

int main()
{
    int n;
    cout<<"enter size of the array";
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int target ;
    cout<<"enter target";
    cin>>target;
    for(int i=0;i<n;i++){
        
            if(a[i]+a[i-1]==target){
                cout<<i<<" "<<i-1<<endl;
            }
              
}
        
    }
 
